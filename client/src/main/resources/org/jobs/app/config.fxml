<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<HBox alignment="CENTER" maxHeight="1.7976931348623157E308"
      maxWidth="1.7976931348623157E308" minHeight="-Infinity"
      minWidth="-Infinity" xmlns="http://javafx.com/javafx/17.0.2-ea"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.jobs.pages.ConfigController">
    <VBox alignment="CENTER" maxHeight="1.7976931348623157E308"
          maxWidth="1.7976931348623157E308" prefHeight="400.0" prefWidth="400.0"
          spacing="32.0">
        <children>
            <VBox alignment="CENTER_LEFT" maxHeight="1.7976931348623157E308"
                  maxWidth="1.7976931348623157E308" spacing="8.0">
                <children>
                    <Label text="IP"/>
                    <TextField fx:id="ip"/>
                </children>
            </VBox>
            <VBox alignment="CENTER_LEFT" maxHeight="-Infinity"
                  maxWidth="1.7976931348623157E308" spacing="8.0">
                <children>
                    <Label text="Porta"/>
                    <TextField fx:id="port"/>
                </children>
            </VBox>
            <Button fx:id="saveButton" maxWidth="1.7976931348623157E308"
                    mnemonicParsing="false" onAction="#onSaveConfig"
                    text="Salvar"/>
        </children>
        <padding>
            <Insets bottom="32.0" left="32.0" right="32.0" top="32.0"/>
        </padding>
    </VBox>
</HBox>
